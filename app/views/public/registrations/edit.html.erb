<h2>会員情報編集</h2>

<%= form_with url: customer_registration_path, method: :put do |f| %>
  <%= render "public/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="actions">
    <%= f.submit "編集" %>
  </div>
<% end %>

<%= link_to "マイページへ戻る", :back %>
